{
  "name": "@claudeforge/orchestrator",
  "version": "1.0.0",
  "description": "Autonomous Development Orchestrator for Claude Code - Spec to Production Pipeline",
  "author": {
    "name": "ClaudeForge",
    "url": "https://github.com/claudeforge"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/claudeforge/orchestrator"
  },
  "license": "MIT",
  "keywords": [
    "claude-code",
    "orchestrator",
    "autonomous-development",
    "multi-agent",
    "ai-development",
    "automation",
    "self-healing",
    "quality-gates"
  ],
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
